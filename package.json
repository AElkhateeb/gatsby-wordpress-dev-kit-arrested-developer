{
  "name": "gatsby-wordpress-dev-kit",
  "version": "1.0.0",
  "description": "A template repo for a development environment using a dockerised Wordpress instance and Gatsby's Wordpress starter",
  "main": "index.js",
  "scripts": {
    "cms:start": "npm run dev:start --prefix cms",
    "cms:stop": "npm run dev:stop --prefix cms",
    "client:init": "gatsby new client https://github.com/GatsbyCentral/gatsby-starter-wordpress",
    "client:start": "npm run start --prefix client",
    "client:build": "npm run install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arrested-developer/gatsby-wordpress-dev-kit.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arrested-developer/gatsby-wordpress-dev-kit/issues"
  },
  "homepage": "https://github.com/arrested-developer/gatsby-wordpress-dev-kit#readme"
}
